@{
}
@model List<Prof>

<div class="container mt-5">
    <h2>List Profs</h2>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">First name</th>
                <th scope="col">Last name</th>
                <th scope="col">Cours designation</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var prof in Model)
            {
                <tr>
                    <td>@prof.Prenom</td>
                    <td>@prof.Nom</td>
                    <td>@prof.Cours?.Designation</td>
                </tr>
            }
        </tbody>
    </table>
</div>